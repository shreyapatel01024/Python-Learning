# Inheritance is one class(child/derived) derives the property and methods of another class(parent/base).

class car:
    @staticmethod
    def start():
        print("car started!!!")
    def stop():
        print("car stoped!!!")
class carname(car):    #properties of car inherited in carname
    def __init__(self,name):        
        self.name=name
car1=carname("Fortuner")
print(car1.start())



 # Output: car started!!!
